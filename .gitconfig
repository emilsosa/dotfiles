[alias]
	co = checkout
	checkout = co
	status = st
	br = branch
	st = status
	merge = merge --no-edit
[user]
	name = Emil Sosa
	email = emiil.sosa@gmail.com
[core]
	mergeoptions = --no-edit

[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[init]
	defaultBranch = main
